function recordscore()
{ 
  // http://www.geeksanon.com/scoreboard/scoreboard.php?gamename=BoltAction
  
  if (playername == "" || playername == "      "){playername = "player";} // Check name
  
  // Game over  
  SetViewport(120, 0);
  Lose.SetPosition(480, 220);
  if (cPrototype.View != 265){Lose.Visible = true;}
  
  // Record name and score LOCAL
  if (File.Exists("local.dat"))
  {
    File *O = File.Open("local.dat", eFileAppend);
    O.WriteRawLine(String.Format("%s", playername));
    int hideround = round + 8888;
    O.WriteRawLine(String.Format("%d", hideround));
    if (game.score < 10){game.score = 10;}
    int stupidencrypt = (game.score * round);
    O.WriteRawLine(String.Format("%d", stupidencrypt));
    O.Close();
  }
  
  // Record name and score WORLD
  if (File.Exists("world.bat"))
  {
    File.Delete("world.bat");
    File *output = File.Open("world.bat", eFileRead);
    output = File.Open("world.bat", eFileWrite);
    output.Close();
  }
  // scoreboard.exe put,BoltAction,b1feb05bef7123d51e0776e81b194729,PLAYER NAME,SCORE,ZZ
  File *W = File.Open("world.bat", eFileAppend);
  W.WriteRawLine(String.Format("scoreboard.exe put,BoltAction,b1feb05bef7123d51e0776e81b194729,%s,%d,ZZ",playername,game.score));
  W.Close();
  
  // Have the scientist come out and talk trash about how you did
  Scientist.ChangeRoom(2, 510, 468);
  Door.Animate(0, 2, eOnce, eBlock, eForwards);
  Scientist.Transparency = 0;
  Scientist.ChangeView(3);
  String title = "FNG";
  if (ProtoCharmer == true){title = "Wiseguy";}
  else if (ProtoGunner == true){title = "Marksman";}
  else if (ProtoSamurai == true){title = "Samurai";}
  else if (ProtoSpeedster == true){title = "Athlete";}
  else if (ProtoToughguy == true){title = "Tough Guy";}
  Scientist.Think(String.Format("Hey %s  You scored %d points and made it to round %d as a %s", playername, game.score, round, title));
  if (ScientistKill == true){if (Random(1) == 1){Scientist.Think("Also, taking a potshot at me was a bad idea");} else {Scientist.Think("Also, trying to kill me was ill-advised");}}
  if (cPrototype.View == 265){if (Random(1) == 1){Scientist.Think("...And good show taking out The Boss!");} else {Scientist.Think("...And nice job offing The Boss!");}}
  if (Boss.Room == 2 && Boss.View != 175){if (Random(1) == 1){Scientist.Think("...And The Boss - AKA Your Clone Brother - won");} else {Scientist.Think("...And your Brother - AKA The Boss - won");}}
  if (ScientistKill == true && Boss.Room == 2 && Boss.View == 340)
  { 
    Autocannon1.FaceCharacter(Boss, eBlock); // Vickers MK 10 Autocannon movement follows The Boss
    cBBullet.ManualScaling = true;
    cBBullet.Scaling = 70;
    cBBullet.Tint(0, 0, 0, 50, 10);
    cBBullet.ChangeRoom(2, 906, 243);
    aGGun_windup_01.Play(eAudioPriorityHigh);
    Wait(10);
    aGGun_SingleShot_01.Play(eAudioPriorityHigh);
    cBBullet.Move(Boss.x, Boss.y, eBlock, eAnywhere);
    Wait(15);
    cBBullet.ChangeRoom(-1);
    Boss.LockViewFrame(175, 1, 3);
    Wait(8);
    Boss.LockViewFrame(175, 1, 4);
    Scientist.Think("...And now The Boss loses as well...");
    Wait(50);
  }
  Scientist.Transparency = 100;
  Door.Animate(0, 1, eOnce, eBlock, eBackwards);
  aDoorslam.Play(eAudioPriorityVeryHigh);
  if (cPrototype.View == 265)
  {
    if (Frank.Room == 2){Frank.LockView(220);}
    Game.SetAudioTypeVolume(eAudioTypeMusic, 20, eVolChangeExisting);
    Wait(20);
    cPrototype.ChangeRoom(4); // Go to Credits!
  }
  else
  {
    ReleaseViewport();
    Lose.Visible = false;
    SetViewport(cPrototype.x, cPrototype.y);
    Wait(250);
    RestartGame(); // Lose!
  }
}

