function musicselect() // Play is musictoggle = false
{
  // Removed YS's Winter Theme (too mellow)
  int musicrand = Random(11); 
  
  if (musicrand == 0 || musicrand == 1){aBroken_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 2 || musicrand == 3){aKiss_me_quick_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 4 || musicrand == 5){aAnticipation_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 6){aCaravan_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 7){aEmptycity_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 8){aOutthere_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 9){aMenace_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 10){aZombiesAreComing_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else if (musicrand == 11){aPressure_rounds.PlayQueued(eAudioPriorityVeryLow);}
  else {aCaravan_rounds.PlayQueued(eAudioPriorityVeryLow);}
}

