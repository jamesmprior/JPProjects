      Scientist.Animate(1, 20, eOnce, eBlock, eForwards);
    }
  }
  else if (sciencerun == 15) // Scientist allows NPC Frank to break loose or Prototype taunts
  {
    Scientist.ChangeView(46); Scientist.LockViewFrame(46, 1, 8); Scientist.Transparency = 0;
    if (twoplayer == false && NPCFrankWalk == false && npcfrank == true) // Send out Frank, unless he is already on the loose or is the second player
    {
      npcfrank = false;
      if (Random(1) == 1){Scientist.Think("One of our top scientists (Doctor Greenboy) created a slow, but powerful zombie-masher to help...Meet FRANK!");}
      else {Scientist.Think("Thanks to Dr. Greenboy, FRANK's here to help!  Use him like a lead blocker.");}
      NPCFrankWalk = true; // NPC Frank is on
      Frank.Scaling = 90;
      Rightdoor.Visible = true;
      Frank.ChangeRoom(2, 605, 425);
      Frank.on = true;
      Frank.LockViewFrame(109, 4, 0); // Frank Ducks
      Wait(10);
      Frank.LockView(109);
      Frank.Animate(3, 4, eOnce, eBlock, eBackwards);
      Frank.Animate(3, 7, eOnce, eBlock, eForwards);
      Wait(70);
      Frank.UnlockView();
      Frank.Walk(FrankX, FrankY, eNoBlock, eWalkableAreas); // Send Frank
      Rightdoor.Visible = false;
    }
    else // Prototype taunts instead
    {
      if (Random(1) == 1){Scientist.Think("I'm getting bored watching you and Frank kill zombies from the safety of this comfortable bunker.");}
      else {Scientist.Think("Take your time guys...no rush...we are only in THE MIDDLE OF A ZOMBIE APOCALYPSE!");}
      SetViewport(Frank.x, Frank.y);
      cPrototype.LockView(6);
      if (Random(1) == 1){cPrototype.Animate(1, 8, eOnce, eBlock); cPrototype.Animate(2, 8, eOnce, eBlock);}
      else {cPrototype.Animate(Random(5) + 4, 10, eOnce, eBlock);}
      ReleaseViewport();
    }
  }
  // Scientist leaves
  Scientist.Transparency = 100;
  Door.Animate(0, 1, eOnce, eBlock, eBackwards);
  aDoorslam.Play(eAudioPriorityVeryHigh);
  ReleaseViewport();
  Game.SetAudioTypeVolume(eAudioTypeMusic, 40, eVolChangeExisting);
}

